@article{bers:1996,
	abstract = {This paper presents a body model server (BMS) that provides
	            real-time access to the position and posture of a person's torso,
	            arms, hands, head, and eyes. The BMS functions as a data-layer
	            between the sensing devices and client applications that require
	            real-time human motion data, such as animation control and
	            multi-modal interfaces. By maintaining an abstract representation
	            of the user's body within the system we remove device dependency
	            from the client interface. For data collection, the model uses four
	            magnetic position/orientation sensors, two data-gloves, and an
	            eye-tracker. The BMS combines the data-streams from the sensors and
	            transforms them into snap-shots of the user's upper-body pose. A
	            geometric model made up of joints and segments structures the
	            input. Posture of the body is represented by joint angles. Two
	            unique characteristics of our approach are: the use of the implicit
	            , geometric constraints of the sensed body to simplify the
	            computation of the unmeasured joint angles, and the use of
	            time-stamped data that allows synchronization with other data
	            streams, e.g. speech input. The working server provides clients
	            with accurate information at up to 40 Hz over a local area network.
	            This paper describes the architecture of the BMS, including the
	            management of multiple input devices, the representation and
	            computation of the position and joint angle data, and the
	            client-server interface.},
	author = {Bers, Josh},
	bibsource = {DBLP, http://dblp.uni-trier.de},
	date-added = {2015-10-13 15:04:34 +0000},
	date-modified = {2015-10-13 15:04:35 +0000},
	journal = {Presence},
	number = {4},
	pages = {381-392},
	title = {A Body Model Server for Human Motion Capture and Representation},
	volume = {5},
	year = {1996},
	Bdsk-File-1 = {
	               
	               },
}

