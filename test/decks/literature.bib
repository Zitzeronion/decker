@inproceedings{gilboa:2011a,
	abstract = {The objective of this study was to describe the nature of the
	            attention deficits in children with NF1 in comparison with
	            typically developed children using the Virtual Classroom (VC).
	            Twenty nine NF1 children and 25 age-and gender-matched controls,
	            aged 8 {\^a}€``16 were assessed in a VC. Parent ratings on the
	            Conners' Parent Rating Scales-Revised; Long (CPRS-R:L)
	            questionnaire was used to monitor for ADHD. Significant differences
