@inproceedings{villar:2018a,
	abstract = {We present Project Zanzibar: a flexible mat that can locate,
	            uniquely identify and communicate with tangible objects placed on
	            its surface, as well as sense a user's touch and hover hand
	            gestures. We describe the underlying technical contributions:
	            efficient and localised Near Field Communication (NFC) over a large
	            surface area; object tracking combining NFC signal strength and
	            capacitive footprint detection, and manufacturing techniques for a
	            rollable device form-factor that enables portability, while
	            providing a sizable interaction area when unrolled. In addition, we
	            detail design patterns for tangibles of varying complexity and
	            interactive capabilities, including the ability to sense
	            orientation on the mat, harvest power, provide additional input and
	            output, stack, or extend sensing outside the bounds of the mat.
	            Capabilities and interaction modalities are illustrated with
	            self-generated applications. Finally, we report on the experience
	            of professional game developers building novel physical/digital
	            experiences using the platform.},
	acmid = {3174089},
	address = {New York, NY, USA},
	articleno = {515},
