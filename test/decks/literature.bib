@article{ernst:2004a,
	abstract = {To perceive the external environment our brain uses multiple
	            sources of sensory information derived from several different
	            modalities, including vision, touch and audition. All these
	            different sources of information have to be efficiently merged to
	            form a coherent and robust percept. Here we highlight some of the
	            mechanisms that underlie this merging of the senses in the brain.
	            We show that, depending on the type of information, different
	            combination and integration strategies are used and that prior
	            knowledge is often required for interpreting the sensory signals. },
	author = {Marc O. Ernst and Heinrich H. B{\"u}lthoff},
