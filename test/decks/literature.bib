@article{325250,
	abstract = {Detail enhances the visual richness and realism of
	            computer-generated images. Our stochastic modelling approach,
	            called particle systems, builds complex pictures from sets of
	            simple, volume-filling primitives. For example, structured particle
	            systems have been used to generate trees and a grass-covered forest
	            floor. Particle systems can produce so much irregular,
	            three-dimensional detail that exact shading and visible surface
	            calculations become infeasible. We describe approximate and
	            probabilistic algorithms for shading and the visible surface
	            problem. Because particle systems algorithms generate
	            richly-detailed images, it is hard to detect any deviation from an
	            exact rendering. Recent work in stochastic modelling also enables
	            us to model complex motions with random variation, such as a field
	            of grass blowing in the breeze. We analyze the performance of our
	            current algorithms to understand the costs of our stochastic
	            modelling approach. },
	address = {New York, NY, USA},
	annote = {You have to read this, it's awesome. Not that it introduces anything
	          especially new or revolutionary, but just to see how they
	          procedurally built up this whole forest. Also this introduces a novel
	          analytical approach for shadowing/AO inside trees, which has been
	          much referenced since.},
	author = {Reeves, William T. and Blau, Ricki},
	date-added = {2015-10-13 15:04:34 +0000},
