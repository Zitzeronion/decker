@article{fischer:2010a,
	abstract = {A recent development in video games is that players can design and
	            personalize their own in-game characters. It was predicted that
	            this innovation could lead to elevations in the intensity of the
	            psychological effects of video games. The present study confirmed
	            this hypothesis, revealing that participants who played an
	            aggressive video game using their own, personalized character
	            exhibited higher levels of aggressive behavior than participants
	            who played an aggressive game with a non-personalized character.
	            The aggressive behavior levels of the own-character players also
	            exceeded those of individuals who played a non-aggressive game,
	            regardless of whether or not they used a personalized character.
	            Process analyses revealed that participants playing a violent video
	            game with a personalized game character experienced more arousal
	            and self-activation than they did when playing with an impersonal,
	            default game character, which in turn increased aggressive
	            responses.},
	annote = {Zitiert im SZ Artikel vom 1.3.2013 Probanden zeigen sehr viel mehr
	          Hilfsbereitschaft und Zivilcourage nachdem prosoziale Spiele wie
	          Lemmings gespielt wurden. Sie griffen z.B. eher ein, wenn eine
	          Versuchsleiterin von einem vermeintlichen Exfreund bel{\"a}stigt
	          wurde.},
	author = {Peter Fischer and Andreas Kastenm{\~A}¼ller and Tobias Greitemeyer},
	date-added = {2015-10-13 15:04:34 +0000},
	date-modified = {2015-10-13 15:04:35 +0000},
	doi = {10.1016/j.jesp.2009.06.010},
	issn = {0022-1031},
	journal = {Journal of Experimental Social Psychology},
	keywords = {Identification, games effects},
	number = {1},
	pages = {192 - 195},
	title = {Media violence and the self: The impact of personalized gaming
	         characters in aggressive video games on aggressive behavior},
	url = {http://www.sciencedirect.com/science/article/pii/S0022103109001395},
	volume = {46},
	year = {2010},
	Bdsk-Url-1 = {
	              http://www.sciencedirect.com/science/article/pii/S0022103109001395
	              },
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.jesp.2009.06.010},
}

